{
  "name": "webapplate",
  "tags": {
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["README.md", "public/js", "server.js", "routes"],
    "exclude": []
  },
  "plugins": [
    "plugins/markdown",
    "plugins/summarize"
  ],
  "opts": {
    "private": true,
    "recurse": true,
    "destination": "docs/"
  }
}
